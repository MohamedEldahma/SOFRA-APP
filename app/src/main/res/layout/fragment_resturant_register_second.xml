<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/ic_background_white"

    tools:context=".ui.frgment.resturant.loginResturant.ResturantRegisterSecondFragment">

    <!--<TextView-->
    <!--android:id="@+id/text_category"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="@dimen/album_dp_50"-->
    <!--android:layout_marginTop="@dimen/album_dp_20"-->
    <!--android:layout_marginStart="@dimen/album_dp_20"-->
    <!--android:layout_marginEnd="@dimen/album_dp_20"-->

    <!--/>-->


    <RelativeLayout
        android:id="@+id/Layout2_types"
        android:layout_width="match_parent"
        android:layout_height="@dimen/album_dp_50"
        android:layout_marginTop="@dimen/album_dp_10"
        android:layout_marginStart="@dimen/album_dp_20"
        android:layout_marginEnd="@dimen/album_dp_20"
        android:background="@color/colordark_gray"
        >



    <com.example.pro.sofranewapp.helper.MultiSelectionSpinner
        android:id="@+id/rest_types_sp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layoutDirection="rtl"    />


    </RelativeLayout>

    <EditText
    android:id="@+id/minimumorder_resturant"
    android:layout_width="match_parent"
    android:layout_height="@dimen/album_dp_50"
    android:layout_marginEnd="@dimen/album_dp_20"
    android:layout_marginStart="@dimen/album_dp_20"
    android:layout_marginTop="@dimen/album_dp_10"
    android:background="@drawable/ic_background_white"
    android:hint="@string/string_minimumorder"
    android:inputType="textPersonName"
    android:padding="@dimen/album_dp_10" />

    <EditText
    android:id="@+id/delivryfee_resturant"
    android:layout_width="match_parent"
    android:layout_height="@dimen/album_dp_50"
    android:background="@drawable/ic_background_white"
    android:layout_marginStart="@dimen/album_dp_20"
    android:layout_marginEnd="@dimen/album_dp_20"
    android:layout_marginTop="@dimen/album_dp_10"
    android:padding="@dimen/album_dp_10"
    android:hint="@string/string_delivryfe"
    android:inputType="textEmailAddress"

    />



    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/string_contact_data"
    android:textSize="@dimen/album_sp_20"
    android:textStyle="bold"
    android:textColor="@color/colorBlack"
    android:layout_marginEnd="@dimen/album_dp_50"
    android:layout_marginTop="@dimen/album_dp_30"
    android:layout_gravity="end"
    />


    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="@dimen/album_dp_50"
    android:orientation="horizontal"
    android:layout_marginTop="@dimen/album_dp_10"
    android:layout_marginStart="@dimen/album_dp_20"
    android:layout_marginEnd="@dimen/album_dp_20"
    android:gravity="center"     >

    <EditText
    android:id="@+id/phonen_umber_resturant"
    android:background="@drawable/ic_background_white"
    android:layout_width="150dp"
    android:layout_height="match_parent"
    android:layout_marginEnd="@dimen/album_dp_10"
    android:layout_marginStart="@dimen/album_dp_20"
    android:paddingEnd="@dimen/album_dp_20"
    android:hint="@string/string_phone"

    />

    <EditText
    android:id="@+id/phone_whatsupo_registe"
    android:layout_width="150dp"
    android:layout_height="match_parent"
    android:background="@drawable/ic_background_white"
    android:padding="@dimen/album_dp_10"
    android:layout_marginEnd="@dimen/album_dp_20"
    android:layout_marginStart="@dimen/album_dp_10"
    android:hint="@string/string_whatsup"

    />
    </LinearLayout>

    <LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:layout_gravity="start"
    android:layout_marginEnd="@dimen/album_dp_40"
    android:layout_marginStart="@dimen/album_dp_20"
    android:layout_marginTop="@dimen/album_dp_20"
    >
    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/string_textphoto"
    android:layout_marginTop="@dimen/album_dp_20"
    android:layout_marginRight="@dimen/album_dp_20"
    android:layout_marginLeft="@dimen/album_dp_10"
    android:textSize="@dimen/album_sp_16"
    android:textColor="@color/colorBlack"
    />

    <ImageView
    android:id="@+id/shop_photo"
    android:layout_width="70dp"
    android:layout_height="70dp"
    android:background="#e7e6e6"
    android:src="@drawable/productphoto"
    />

    </LinearLayout>

    <Button
    android:id="@+id/registersecond_resturantsecond"
    android:layout_width="match_parent"
    android:layout_height="@dimen/album_dp_50"
    android:background="@color/colorgreen"
    android:layout_marginStart="@dimen/album_dp_20"
    android:layout_marginEnd="@dimen/album_dp_20"
    android:layout_marginTop="@dimen/album_dp_30"
    android:text="@string/string_sinup"
    />




</LinearLayout>